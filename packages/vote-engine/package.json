{
	"name": "@votetorrent/vote-engine",
	"version": "0.0.1",
	"type": "module",
	"description": "Concerete implementation of voting functionality for VoteTorrent",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"files": [
		"src",
		"dist",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "tsc -p tsconfig.build.json",
		"lint": "yarn lint",
		"test": "TS_NODE_PROJECT=./tsconfig.test.json mocha --node-option=import=./register-ts-node.mjs --node-option=experimental-specifier-resolution=node --require tsconfig-paths/register \"test/**/*.spec.ts\"",
		"dep-check": "yarn dep-check"
	},
	"homepage": "https://github.com/gotchoices/votetorrent",
	"bugs": {
		"url": "https://github.com/gotchoices/votetorrent/issues"
	},
	"keywords": [
		"vote",
		"voting"
	],
	"license": "MIT",
	"peerDependencies": {
		"@react-native-async-storage/async-storage": "^2.1.1",
		"react-native": "0.78.0"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.13.10",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"@babel/core": "^7.25.2",
		"@babel/runtime": "^7.25.0",
		"@noble/curves": "^1.9.6",
		"@noble/hashes": "^1.8.0",
		"@optimystic/quereus-plugin-crypto": "0.2.0",
		"@quereus/quereus": "^0.4.11",
		"@react-native-async-storage/async-storage": "^2.1.2",
		"@votetorrent/vote-core": "^0.0.1",
		"chai": "^5.2.0",
		"mocha": "^11.5.0",
		"react": "19.0.0",
		"react-native": "0.78.0",
		"temporal-polyfill": "^0.3.0"
	},
	"ts-node": {
		"esm": true,
		"experimentalSpecifierResolution": "node"
	}
}
